      <options>
        <option name="unsigned"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="etat" type="string" column="etat" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="dateReception" type="string" column="date_reception" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="idMagasin" type="integer" column="id_magasin" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="idCommande" type="integer" column="id_commande" nullable="true">
      <options>
        <option name="unsigned"/>
