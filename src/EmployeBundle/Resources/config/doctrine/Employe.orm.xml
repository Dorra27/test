<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EmployeeBundle\Entity\Employe" table="employe">
    <id name="idEmp" type="integer" column="ID_emp">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="USERNAME" length="180" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="PRENOM" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="EMAIL" length="180" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresse" type="string" column="ADRESSE" length="1000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateNaissance" type="date" column="DATE_NAISSANCE" nullable="false"/>
    <field name="cin" type="string" column="CIN" length="8" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="roles" type="text" column="ROLES" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mission" type="string" column="MISSION" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telephone" type="integer" column="telephone" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="governat" type="string" column="governat" length="20" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="password" type="string" column="password" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="disponible" type="string" column="disponible" length="50" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="id" type="integer" column="id" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">NULL</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
