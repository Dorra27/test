<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EmployeeBundle\Entity\VehiculeUser" table="vehicule_user">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="matricule" type="string" column="matricule" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idUser" type="integer" column="id_user" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateDebut" type="date" column="date_debut" nullable="true">
      <options>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="dateFin" type="date" column="date_fin" nullable="true">
      <options>
        <option name="default">NULL</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
