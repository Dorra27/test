<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EmployeeBundle\Entity\MaintenaceVehicule" table="maintenace_vehicule">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nature" type="string" column="nature" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="date" type="date" column="date" nullable="true">
      <options>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cout" type="integer" column="cout" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="matricule" type="string" column="matricule" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
