<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EmployeeBundle\Entity\Depot" table="depot">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="adresse" type="string" column="adresse" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="surface" type="integer" column="surface" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idproduit" type="integer" column="idproduit" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="prix" type="integer" column="prix" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="etat" type="string" column="etat" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="datedebut" type="date" column="datedebut" nullable="true">
      <options>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="datefin" type="date" column="datefin" nullable="true">
      <options>
        <option name="default">NULL</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
