<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EmployeeBundle\Entity\Produit" table="produit">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="photo" type="string" column="photo" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="poids" type="integer" column="poids" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="prix" type="integer" column="prix" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="etat" type="string" column="etat" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="desription" type="string" column="desription" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idcat" type="integer" column="idcat" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="idDepot" type="integer" column="id_depot" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="libelle" type="string" column="libelle" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="quantite" type="integer" column="quantite" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idUser" type="integer" column="id_user" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">NULL</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
