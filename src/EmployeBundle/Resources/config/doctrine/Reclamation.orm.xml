<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EmployeeBundle\Entity\Reclamation" table="reclamation">
    <id name="idRec" type="integer" column="id_rec">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="objet" type="string" column="objet" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etat" type="integer" column="etat" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="date" type="date" column="date" nullable="false"/>
    <field name="id" type="integer" column="id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="image" type="string" column="image" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
