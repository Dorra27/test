<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EmployeeBundle\Entity\PointCollecte" table="point_collecte">
    <id name="idMagasin" type="integer" column="id_magasin">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="pays" type="string" column="pays" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="log" type="integer" column="log" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="lat" type="integer" column="lat" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="nom" type="string" column="nom" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="horaireTravail" type="string" column="horaire_travail" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="jour" type="string" column="jour" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
