<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EmployeeBundle\Entity\Commande" table="commande">
    <id name="idCommande" type="integer" column="id_commande">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="id" type="integer" column="id" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="date" type="datetime" column="date" nullable="true">
      <options>
        <option name="default">current_timestamp()</option>
      </options>
    </field>
    <field name="adresseDest" type="string" column="adresse_dest" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="total" type="integer" column="total" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
