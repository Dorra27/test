<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EmployeeBundle\Entity\Feedback" table="feedback">
    <id name="idFeed" type="integer" column="id_feed">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="description" type="string" column="description" length="90" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="note" type="integer" column="note" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="id" type="integer" column="id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
