<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EmployeeBundle\Entity\Facture" table="facture">
    <id name="idFacture" type="integer" column="id_facture">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idLivraison" type="integer" column="id_livraison" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="idCommande" type="integer" column="id_commande" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="typePaiement" type="string" column="type_paiement" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="typeFacture" type="string" column="type_facture" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="date" type="date" column="date" nullable="false"/>
  </entity>
</doctrine-mapping>
