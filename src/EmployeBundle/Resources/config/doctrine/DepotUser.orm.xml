<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EmployeeBundle\Entity\DepotUser" table="depot_user">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idDepot" type="integer" column="id_depot" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idUser" type="integer" column="id_user" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="datedebut" type="date" column="datedebut" nullable="true">
      <options>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="datefin" type="date" column="datefin" nullable="true">
      <options>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="etat" type="string" column="etat" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
